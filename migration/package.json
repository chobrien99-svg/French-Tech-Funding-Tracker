{
  "name": "ftj-funding-migration",
  "version": "1.0.0",
  "description": "Migration tool for French Tech Funding Tracker - JSON to Supabase",
  "main": "migrate.js",
  "scripts": {
    "migrate": "node migrate.js",
    "migrate:dry-run": "DRY_RUN=true node migrate.js",
    "match-siren": "node match-siren.js",
    "match-siren:confirm": "node match-siren.js --confirm",
    "match-siren:dry-run": "node match-siren.js --dry-run",
    "match-siren:test": "node match-siren.js --dry-run --limit 5"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "dotenv": "^16.3.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "French Tech Journal",
  "license": "MIT"
}
